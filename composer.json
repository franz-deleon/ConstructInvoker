{
    "name": "franz-deleon/fdl-construct-invoker",
    "type": "library",
    "description": "A ZF2 module that enables argument passing to class constructors (__construct()) that requires or not require arguments at construction time.",
    "keywords": [
        "zf2",
        "zf2 constructor",
        "constructor initialization",
        "adding constructor arguments",
        "Zend Framework"
    ],
    "homepage": "https://github.com/franz-deleon/ConstructInvoker",
    "license": "MIT",
    "authors": [
        {
            "name": "Franz de Leon",
            "email": "franz_dl@yahoo.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": ">=2.2.0",
        "franz-deleon/fdl-module-reloader": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "FdlConstructInvoker\\": "src/"
        }
    }
}